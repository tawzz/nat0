<cards>
  <age1>
    <card name="aryan_migration" age="1" famine="1" architects="2">
      <code>"least military :res book -3"</code>
      <cost res="book" n="3">
        <cond op="least" res="military"/>
      </cost>
      <yield res="wheat" n="3">
        <cond op="all_except_least" res="stability"/>
      </yield>
    </card>
    <card name="assyrian_deportations" age="1" famine="0" architects="1">
      <cost res="worker" n="1">
        <cond op="least" res="stability"/>
      </cost>
      <regain_war_loss res="vp">
        <cond op="most" res="stability"/>
      </regain_war_loss>
    </card>
    <card name="attila" age="1" famine="3" architects="2">
      <cost res="gold" n="3">
        <cond op="least" res="military"/>
      </cost>
      <cost res="wheat" n="3">
        <cond op="all_except_most" res="stability"/>
      </cost>
      <yield res="coal" n="4">
        <cond op="most" res="stability"/>
      </yield>
    </card>
    <card name="bread_and_games" age="1" famine="0" architects="2">
      <yield res="wheat" n="4">
        <cond op="most" res="stability"/>
      </yield>
      <opt players="all">
        <cost res="wheat" n="4"/>
        <yield res="book" n="6"/>
      </opt>
    </card>
    <card name="hellenism" age="1" famine="1" architects="0">
      <yield res="coal" n="2">
        <cond op="most" res="military"/>
      </yield>
      <cost res="wheat" n="2">
        <cond op="all_except_most" res="military"/>
      </cost>
      <opt players="all">
        <undeploy res="military" n="*"/>
        <yield res="book" n="2"/>
      </opt>
    </card>
    <card name="pax_romana" age="1" famine="2" architects="0">
      <yield res="vp" n="1">
        <cond op="most" res="military"/>
      </yield>
      <opt players="all">
        <cond op="most" res="stability"/>
        <yield res="worker" n="1"/>
        <yield res="wheat" n="3"/>
      </opt>
    </card>
    <card name="qin_unification" age="1" famine="1" architects="1">
      <yield res="vp" n="1">
        <cond op="greater_than_war" res="military"/>
      </yield>
      <check op="go_first">
        <cond op="most" res="stability"/>
      </check>
    </card>
    <card name="rigveda" age="1" famine="2" architects="0">
      <yield res="book" n="*">
        <or>
          <cond op="bought" type="war"/>
          <cond op="bought" res="battle"/>
        </or>
      </yield>
      <yield res="wheat" n="4">
        <cond op="most" res="wheat"/>
      </yield>
    </card>
    <card name="sea_peoples" age="1" famine="2" architects="1">
      <cond op="least" res="military">
        <or>
          <cost op="remove" res="architect" n="1"/>
          <cost res="vp" n="1"/>
        </or>
      </cond>
      <cost res="vp" n="1">
        <and>
          <cond op="not_most" res="military"/>
          <cond op="not_most" res="stability"/>
        </and>
      </cost>
      <cost res="wheat" n="2">
        <cond op="all_except_most" res="military"/>
      </cost>

    </card>
